# Environment variables for webhook worker
# Copy this to .env and fill in the values

# Railway API URL (your main Relay API)
API_BASE_URL=https://your-relay-api.up.railway.app
RELAY_API_URL=https://your-relay-api.up.railway.app

# Worker authentication secret (matches server's WORKER_AUTH_SECRET)
API_SECRET=your-worker-auth-secret-here

# X25519 private key for encrypting webhook payloads (generate with: npm run generate-keys)
WORKER_ENCRYPTION_PRIVATE_KEY=

# Ed25519 private key for signing requests to API (generate with: npm run generate-keys)
WORKER_PRIVATE_KEY=

# Port (Railway sets this automatically, defaults to 3000)
PORT=3000
